{
  "env": {
      "node": true,
      "commonjs": true,
      "es6": true,
      "jest": true
  },
  "extends": "eslint:recommended",
  "rules": {
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
  },
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  }
}